{
  "version": 3,
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": { "Location": "/$1" },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": { "Location": "/$1" },
      "status": 308
    },
    { "src": "^/(.*)/$", "headers": { "Location": "/$1" }, "status": 308 },
    {
      "src": "/_app/.+",
      "headers": { "cache-control": "public, immutable, max-age=31536000" }
    },
    { "handle": "filesystem" },
    { "src": "/app-edge/.*", "dest": "/app-edge" }
  ],
  "overrides": {}
}
